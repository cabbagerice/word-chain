  function hide_download() {
    $('#footer').css('z-index', footerZ);
    $('#warning_download').css({'opacity': '0', 'display': 'none'});
    $('#cover_download').css({'display': 'none'});
  }

  function hide_cancel_download() {
      $('#hide_download').hide();
  }

  $(document).ready(function () {
    $("#show_download").click(function() { show_download() });
    $("#hide_download").click(function() { hide_download() });
      $("#cancel_download").click(function() { hide_download() });
  });
</script>


