<script>
  var footerZ = 0;

  function show_flashcards() {
    footerZ = $('#footer').css('z-index');
    $('#footer').css('z-index', -1);
    $('#credits').css('z-index', -1);


    sizeOverlayVarHeight($('#warning_flashcards'));
