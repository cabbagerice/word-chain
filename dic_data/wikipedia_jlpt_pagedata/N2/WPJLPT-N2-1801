        sizeOverlayVarHeight($('#warning_flashcards'));
    });
    $('#warning_flashcards').css({'opacity': '1', 'z-index': '1001'});
    $('#cover_flashcards').css({'opacity': '1','z-index': '1000'}).fadeIn(500);
    $('#cancel_flashcards').css({'z-index': '1002'});
  }

  function hide_flashcards() {
    $('#footer').css('z-index', footerZ);
    $('#warning_flashcards').css({'opacity': '0', 'display': 'none'});
    $('#cover_flashcards').css({'display': 'none'});
  }

  function hide_cancel_flashcards() {
      $('#hide_flashcards').hide();
  }

  $(document).ready(function () {
    $("#show_flashcards").click(function() { show_flashcards() });
    $("#hide_flashcards").click(function() { hide_flashcards() });
      $("#cancel_flashcards").click(function() { hide_flashcards() });
  });
</script>


<div id="cover_flashcards" class="cover"></div>
<div id="warning_flashcards" class="warning" style="display: none; ">
  <a id="hide_flashcards" class="x-hide">&times;</a>

