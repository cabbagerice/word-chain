      <div class="w_title" style="">Favorites</div>

  <div class="w_panel">
    <div class="w_text" id="warning_text_favorites_warning">Please <a href='/users/sign_in'>LOG IN</a> to use
                        Favorites.<br /><br />
                        Wherever you see the <span class='ja-star' style='color: #58c051;'></span> green star,
                        you can tag any kanji, word or grammar point for future reference in your
                        <a href='/favorites'>Favorites</a> list, which is accessible from the main menu.
                         You can automatically create <a href='/srs'>flashcards</a> from your Favorites,
                         or use <a href='/quick'>Quick Study</a> to study them any time.</div>
  </div>


  <div style="clear:both;">&nbsp;</div>
</div>



<div id="ms">
  <a id="ms_hide">&times;</a>
  <div id="ms_head"></div>
  <div id="ms_subhead"></div>
</div>




<script>
  var target;
  function launch() {
    $("#cbhide").click(hideCBuilder);
    $(window).resize(sizeCBuilder);

    

    initCBuilder();

  }
  function setTarget(divId) {
      target = divId;
  }
  function select(kanji) {
    $('#'+target).val($('#'+target).val() + kanji);
    cbreset();
    if (kanji == '𠂉') {
      location.href = encodeURI('/kanji/𠂉');
    } else {
      hideCBuilder();
    }
  }

  $(document).ready(launch);
</script>


<div id="cover"> </div>

