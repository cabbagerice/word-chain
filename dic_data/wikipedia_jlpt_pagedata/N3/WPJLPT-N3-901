  <a id="ms_hide">&times;</a>
  <div id="ms_head"></div>
  <div id="ms_subhead"></div>
</div>




<script>
  var target;
  function launch() {
    $("#cbhide").click(hideCBuilder);
    $(window).resize(sizeCBuilder);

    

    initCBuilder();

  }
  function setTarget(divId) {
      target = divId;
  }
  function select(kanji) {
    $('#'+target).val($('#'+target).val() + kanji);
    cbreset();
    if (kanji == '𠂉') {
      location.href = encodeURI('/kanji/𠂉');
    } else {
      hideCBuilder();
    }
  }

  $(document).ready(launch);
</script>


