<script>
  var target;
  function launch() {
    $("#cbhide").click(hideCBuilder);
    $(window).resize(sizeCBuilder);

    

    initCBuilder();

  }
  function setTarget(divId) {
      target = divId;
  }
  function select(kanji) {
    $('#'+target).val($('#'+target).val() + kanji);
    cbreset();
    if (kanji == '𠂉') {
      location.href = encodeURI('/kanji/𠂉');
    } else {
      hideCBuilder();
    }
  }

  $(document).ready(launch);
</script>


<div id="cover"> </div>

    <div id="cb">

      <div id="components">
        <div id="inputsdiv"><div class="buttondiv"><button id="cbreset" type="submit">Reset
        </button></div><div class="inputdiv"><input type="text" id="ctext" spellcheck="false"
                                                    autocorrect="off" autocapitalize="off" autocomplete="off"
        /></div><div id="helpsettingsdiv"><button id="settingsb" type="submit"
        >Change list</button><div
